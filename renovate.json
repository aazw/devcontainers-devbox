{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": ["devbox"],
      "matchFileNames": ["images/**/devbox.json"],
      "groupName": "devbox ({{parentDir}})",
      "groupSlug": "devbox-{{parentDir}}"
    }
  ]
}
